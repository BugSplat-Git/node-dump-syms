{
  "name": "node-dump-syms",
  "version": "3.0.0",
  "description": "Neon wrapper for Mozilla's dump_syms",
  "main": "index.node",
  "scripts": {
    "start": "node index.js RUST_BACKTRACE=1",
    "prestart": "npm run build-release",
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "@bobbyg603",
  "license": "MIT",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BugSplat-Git/node-dump-syms.git"
  },
  "keywords": [
    "breakpad",
    "crashpad",
    "sym",
    "mozilla",
    "minidump",
    "crash",
    "bugsplat"
  ],
  "bugs": {
    "url": "https://github.com/BugSplat-Git/node-dump-syms/issues"
  },
  "homepage": "https://github.com/BugSplat-Git/node-dump-syms#readme"
}