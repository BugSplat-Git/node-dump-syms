{
  "name": "node-dump-syms",
  "version": "1.2.1",
  "description": "Utility for running Breakpad's dump_syms via Node.js",
  "main": "./dist/index.js",
  "bin": {
    "node-dump-syms": "./dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prestart": "mkdirp ./out",
    "start": "ts-node ./bin/index.ts ./spec/support/darwin/addon.node ./out/addon.node.sym darwin",
    "build": "tsc && copyfiles ./bin/**/* -e ./bin/index.ts dist",
    "test": "ts-node ./node_modules/.bin/jasmine",
    "prerelease": "npm run build",
    "release": "npm publish --access public"
  },
  "keywords": [
    "crashpad",
    "breakpad",
    "symbols",
    "sym",
    "pdb",
    "so",
    "dll",
    "dSYM"
  ],
  "author": "BugSplat",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.11.17",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "jasmine": "^4.5.0",
    "mkdirp": "^2.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
